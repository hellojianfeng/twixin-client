{
  "name": "twixin",
  "version": "1.0.0",
  "description": "A notification app for your rich notifications",
  "repository": {
    "type": "git",
    "url": "https://github.com/twixin/twixin.git"
  },
  "keywords": [
    "Twixin notification"
  ],
